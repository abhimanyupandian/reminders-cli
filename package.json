    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-0": "^6.22.0",
    "eslint": "^3.19.0",
